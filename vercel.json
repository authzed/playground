{
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000;"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' $HEADER_CONTENT_SECURITY_POLICY;"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache;"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' $HEADER_CONTENT_SECURITY_POLICY;"
        }
      ]
    },
    {
      "source": "/config.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache;"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' $HEADER_CONTENT_SECURITY_POLICY;"
        }
      ]
    },
    {
      "source": "/config-env.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache;"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' $HEADER_CONTENT_SECURITY_POLICY;"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' $HEADER_CONTENT_SECURITY_POLICY;"
        }
      ]
    }
  ]
}
